{
    "name": "redocus",
    "version": "0.2.0",
    "description": "A tiny static site generator using React and MDX",
    "author": {
        "name": "Josemi Juanes",
        "email": "hello@josemi.xyz"
    },
    "license": "MIT",
    "repository": "https://github.com/jmjuanes/redocus",
    "bugs": "https://github.com/jmjuanes/redocus/issues",
    "bin": {
        "redocus": "index.js"
    },
    "scripts": {
        "docs": "yarn docs:build",
        "docs:build": "node ./index.js --config redocus.config.js"
    },
    "keywords": [
        "static",
        "site",
        "docs",
        "react",
        "mdx"
    ],
    "files": [
        "README.md",
        "LICENSE",
        "index.js",
        "theme.js"
    ],
    "dependencies": {
        "@babel/core": "^7.23.7",
        "@babel/preset-react": "^7.23.3",
        "@babel/register": "^7.23.7",
        "gray-matter": "^4.0.3"
    },
    "devDependencies": {
        "@mdx-js/mdx": "^3.0.1",
        "lowcss": "^0.19.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "peerDependencies": {
        "@mdx-js/mdx": ">=3.0.0",
        "lowcss": ">=0.19.0",
        "react": ">=18.2.0",
        "react-dom": ">=18.2.0"
    }
}
